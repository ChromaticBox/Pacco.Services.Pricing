{
  "app": {
    "name": "Pacco Pricing Service"
  },
  "consul": {
    "enabled": true,
    "url": "http://localhost:8500",
    "service": "pricing-service",
    "address": "docker.for.win.localhost",
    "port": "5008",
    "pingEnabled": true,
    "pingEndpoint": "ping",
    "pingInterval": 3,
    "removeAfterInterval": 3
  },
  "fabio": {
    "enabled": true,
    "url": "http://localhost:9999",
    "service": "pricing-service"
  },
  "httpClient": {
    "type": "fabio",
    "retries": 2,
    "services": {
      "customers": "customers-service"
    }
  },
  "logger": {
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": false,
      "path": "logs/logs.txt",
      "interval": "day"
    }
  }
}